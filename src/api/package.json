{
  "name": "@storify/api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit",
    "clean": "rm -rf dist && docker-compose down && rm -rf .dbdata",
    "watch": "tsc -w",
    "dev": "esr src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "db:setup": "ts-node ./src/test/setupDb.ts",
    "test": "cross-env NODE_ENV=test npm run db:setup && jest",
    "test:watch": "cross-env NODE_ENV=test npm run db:setup && jest --watch",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "migrate:gen": "sh scripts/migrate.sh",
    "migrate:up": "typeorm migration:run",
    "seed:db": "sh scripts/seed-db.sh"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.0.1",
    "apollo-server-cache-redis": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "aws-sdk": "^2.991.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "1.27.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.0.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "ioredis": "^4.25.0",
    "jest": "^27.0.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "stripe": "^8.195.0",
    "ts-jest": "^27.0.3",
    "type-graphql": "^1.1.1",
    "typeorm": "0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.2",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.6",
    "@types/ioredis": "^4.22.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^17.0.5",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "gen-env-types": "^1.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "10.4.0",
    "typescript": "^4.3.2"
  }
}
